                </rightValue>
            </conditions>
            <conditions>
                <leftValueReference>$Record.Cloud_Contract_Type__c</leftValueReference>
                <operator>EqualTo</operator>
                <rightValue>
                    <stringValue>EDP Shareshift</stringValue>
                </rightValue>
            </conditions>
            <conditions>
                <leftValueReference>$Record.Cloud_Contract_Type__c</leftValueReference>
                <operator>EqualTo</operator>
                <rightValue>
                    <stringValue>EDP Non Shareshift</stringValue>
                </rightValue>
            </conditions>
            <conditions>
                <leftValueReference>$Record.Onica_Type__c</leftValueReference>
                <operator>EqualTo</operator>
