    <rules>
        <fullName>Email to Creator On assigned to Admin</fullName>
        <actions>
            <name>Email_to_Creator_when_assigned_to_admin</name>
            <type>Alert</type>
        </actions>
        <active>true</active>
        <formula>AND(
ISCHANGED( OwnerId ), 
NOT(ISBLANK( Owner:User.Id )),
(CreatedBy.LastName &lt;&gt; &quot;System&quot;),
CASESAFEID(OwnerId ) &lt;&gt; CASESAFEID( CreatedById )
)</formula>
        <triggerType>onAllChanges</triggerType>
    </rules>
