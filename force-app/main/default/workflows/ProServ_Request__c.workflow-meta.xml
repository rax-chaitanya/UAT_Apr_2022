    <rules>
        <fullName>Confirm to Seller that ProServ Request has been created</fullName>
        <actions>
            <name>Confirmation_Email_of_ProServ_Request</name>
            <type>Alert</type>
        </actions>
        <active>true</active>
        <criteriaItems>
            <field>ProServ_Request__c.CreatedById</field>
            <operation>notEqual</operation>
        </criteriaItems>
        <triggerType>onCreateOnly</triggerType>
    </rules>
    <rules>
        <fullName>Notify Seller of assigned PS Specialist</fullName>
        <actions>
            <name>Notify_Seller_that_PS_Specialist_assigned_to_Request</name>
            <type>Alert</type>
        </actions>
        <active>false</active>
        <criteriaItems>
            <field>ProServ_Request__c.PS_Team_Member_Assigned__c</field>
            <operation>notEqual</operation>
        </criteriaItems>
        <description>Send email alert to notify seller that a PS Specialist has been assigned to their request</description>
        <triggerType>onCreateOrTriggeringUpdate</triggerType>
    </rules>
    <rules>
        <fullName>Notify Seller of assigned PS Specialist with Partner Company Onica</fullName>
        <actions>
            <name>Notify_Seller_that_PS_Specialist_assigned_to_Request_for_Partner_Company_Onica</name>
            <type>Alert</type>
        </actions>
        <active>true</active>
        <criteriaItems>
            <field>ProServ_Request__c.Partner_Company__c</field>
            <operation>equals</operation>
            <value>Onica</value>
        </criteriaItems>
        <description>Send email alert to notify seller that a PS Specialist has been assigned to their request with the Parent Company Onica</description>
        <triggerType>onCreateOrTriggeringUpdate</triggerType>
    </rules>
    <rules>
        <fullName>Send Email Notification to PS Team Member on ProServe Request</fullName>
        <actions>
            <name>Notify_PS_Specialist_they_have_been_tagged_on_a_ProServe_Request</name>
            <type>Alert</type>
        </actions>
        <active>true</active>
        <formula>NOT(
				ISNULL( PS_Team_Member_Assigned__c )
				)</formula>
        <triggerType>onCreateOrTriggeringUpdate</triggerType>
    </rules>
    <rules>
        <fullName>US ProServ Request Created</fullName>
        <actions>
            <name>ProServ_Request_Created_Alert_Email</name>
            <type>Alert</type>
        </actions>
        <active>false</active>
        <criteriaItems>
            <field>ProServ_Request__c.CreatedDate</field>
            <operation>notEqual</operation>
        </criteriaItems>
        <criteriaItems>
            <field>User.Region__c</field>
            <operation>notEqual</operation>
            <value>APAC</value>
        </criteriaItems>
        <triggerType>onCreateOnly</triggerType>
    </rules>
</Workflow>
