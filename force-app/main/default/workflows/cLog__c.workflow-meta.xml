    <rules>
        <fullName>CDH_NewSfdcToFusion-DeleteContact</fullName>
        <actions>
            <name>CDH_NewSfdcToFusion_DeleteContact</name>
            <type>OutboundMessage</type>
        </actions>
        <active>true</active>
        <description>Workflow for New Salesforce to FMW Integration  - to notify deleted contacts to Stash</description>
        <formula>AND(  NOT(CONTAINS($Label.SkipUserIDs,$User.Id)), NOT($User.Id=$Label.FMW_API_User), NOT(ISBLANK(RPN__c)), Name = &apos;Contact&apos;)</formula>
        <triggerType>onCreateOnly</triggerType>
    </rules>
