<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>pse__Expense_ER_Project_Mismatch</fullName>
    <active>true</active>
    <description>Expense Line Project must match Expense Report Project.</description>
    <errorConditionFormula>AND(NOT(ISNULL(pse__Project__c)),(pse__Project__c &lt;&gt; &apos;&apos;),NOT(ISNULL(pse__Expense_Report__c)),(pse__Expense_Report__c &lt;&gt; &apos;&apos;),NOT(ISNULL(pse__Expense_Report__r.pse__Project__c)),(pse__Expense_Report__r.pse__Project__c &lt;&gt; &apos;&apos;),(pse__Project__c &lt;&gt; pse__Expense_Report__r.pse__Project__c))</errorConditionFormula>
    <errorDisplayField>pse__Expense_Report__c</errorDisplayField>
    <errorMessage>Expense Line Project must match Expense Report Project.</errorMessage>
</ValidationRule>
