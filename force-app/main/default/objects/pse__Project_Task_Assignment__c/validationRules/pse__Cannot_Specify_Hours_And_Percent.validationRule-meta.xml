<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>pse__Cannot_Specify_Hours_And_Percent</fullName>
    <active>true</active>
    <errorConditionFormula>AND(NOT(ISBLANK( pse__Allocation_Hours__c )),NOT(ISBLANK( pse__Allocation_Percentage__c )))</errorConditionFormula>
    <errorMessage>Cannot apply both Allocation Hours and Allocation Percentage</errorMessage>
</ValidationRule>
