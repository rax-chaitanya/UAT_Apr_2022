<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Support_Reason_Cannot_be_Blank</fullName>
    <active>false</active>
    <errorConditionFormula>AND(ISPICKVAL(Support_Reason__c,&quot;&quot;),CONTAINS(RecordType.Name,&quot;Salesforce help&quot;))</errorConditionFormula>
    <errorDisplayField>Support_Reason__c</errorDisplayField>
    <errorMessage>Support Reason cannot be blank</errorMessage>
</ValidationRule>
