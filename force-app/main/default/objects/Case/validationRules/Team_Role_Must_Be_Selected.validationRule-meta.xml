<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Team_Role_Must_Be_Selected</fullName>
    <active>false</active>
    <errorConditionFormula>AND(OR(ISPICKVAL(Support_Reason__c,&quot;OVERLAY REQUEST&quot;),ISPICKVAL(Support_Reason__c,&quot;ADD COMPANY TEAM MEMBER&quot;)),ISPICKVAL(Team_Role__c,&quot;&quot;))</errorConditionFormula>
    <errorDisplayField>Team_Role__c</errorDisplayField>
    <errorMessage>Team Role must be selected</errorMessage>
</ValidationRule>
