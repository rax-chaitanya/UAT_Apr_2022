<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Provide_User_to_Deactivate</fullName>
    <active>false</active>
    <errorConditionFormula>AND(ISPICKVAL(Support_Reason__c,&quot;DEACTIVATE USER&quot;),ISBLANK( User_to_Deactivate__c ))</errorConditionFormula>
    <errorDisplayField>User_to_Deactivate__c</errorDisplayField>
    <errorMessage>Please provide the User to Deactivate</errorMessage>
</ValidationRule>
