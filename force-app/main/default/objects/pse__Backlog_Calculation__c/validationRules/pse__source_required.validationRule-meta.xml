<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>pse__source_required</fullName>
    <active>true</active>
    <errorConditionFormula>AND(ISBLANK(pse__Region__c), ISBLANK(pse__Practice__c), ISBLANK(pse__Group__c), ISBLANK(pse__Project__c))</errorConditionFormula>
    <errorMessage>One of Region, Practice, Group, or Project is required</errorMessage>
</ValidationRule>
