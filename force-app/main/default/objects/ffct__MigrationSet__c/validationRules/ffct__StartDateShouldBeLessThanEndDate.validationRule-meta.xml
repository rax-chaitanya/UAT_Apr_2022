<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>ffct__StartDateShouldBeLessThanEndDate</fullName>
    <active>true</active>
    <description>The start date should be less end date.</description>
    <errorConditionFormula>ffct__StartDate__c   &gt;   ffct__EndDate__c</errorConditionFormula>
    <errorDisplayField>ffct__StartDate__c</errorDisplayField>
    <errorMessage>The End Date must be later than the Start Date.</errorMessage>
</ValidationRule>
