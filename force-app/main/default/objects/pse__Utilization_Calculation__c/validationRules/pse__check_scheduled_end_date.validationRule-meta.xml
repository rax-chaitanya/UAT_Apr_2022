<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>pse__check_scheduled_end_date</fullName>
    <active>true</active>
    <errorConditionFormula>AND(pse__Calculate_Scheduled_Utilization__c, pse__Scheduled_Utilization_End_Date__c &lt; pse__Scheduled_Utilization_Start_Date__c)</errorConditionFormula>
    <errorDisplayField>pse__Scheduled_Utilization_End_Date__c</errorDisplayField>
    <errorMessage>End date is prior to the start date</errorMessage>
</ValidationRule>
