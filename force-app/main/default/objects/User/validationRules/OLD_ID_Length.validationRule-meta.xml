<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>OLD_ID_Length</fullName>
    <active>false</active>
    <description>18 Characters only</description>
    <errorConditionFormula>IF(NOT(ISBLANK(Old_Record_Id__c)),(LEN(Old_Record_Id__c)&lt; 18),(LEN(Old_Record_Id__c)&gt;0))</errorConditionFormula>
    <errorDisplayField>Old_Record_Id__c</errorDisplayField>
    <errorMessage>Must be 18 characters.</errorMessage>
</ValidationRule>
