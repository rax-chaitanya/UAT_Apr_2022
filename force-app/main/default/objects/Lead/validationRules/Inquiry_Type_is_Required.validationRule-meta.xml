<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Inquiry_Type_is_Required</fullName>
    <active>false</active>
    <errorConditionFormula>AND(
				ISPICKVAL(
         EMEA_Inquiry_Type_Product__c,&quot;&quot;),
				ISPICKVAL(
								$User.Region__c,&quot;EMEA&quot;)
				)</errorConditionFormula>
    <errorDisplayField>EMEA_Inquiry_Type_Product__c</errorDisplayField>
    <errorMessage>Inquiry Type must be selected</errorMessage>
</ValidationRule>
