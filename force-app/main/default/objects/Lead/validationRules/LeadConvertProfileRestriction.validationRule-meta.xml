<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>LeadConvertProfileRestriction</fullName>
    <active>true</active>
    <errorConditionFormula>AND( IsConverted,  CONTAINS( $Profile.Id ,$Label.LeadConvertEM) )</errorConditionFormula>
    <errorMessage>You do not have permission to convert the lead</errorMessage>
</ValidationRule>
