<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Inquiry_Focus_is_Required</fullName>
    <active>false</active>
    <errorConditionFormula>AND(
				ISPICKVAL(
         EMEA_Inquiry_Focus_Solution__c,&quot;&quot;),
				ISPICKVAL(
								 $User.Region__c,&quot;EMEA&quot;)
				)</errorConditionFormula>
    <errorDisplayField>EMEA_Inquiry_Focus_Solution__c</errorDisplayField>
    <errorMessage>Inquiry Focus must be selected</errorMessage>
</ValidationRule>
